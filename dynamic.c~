#include <stdio.h>

struct Shape {
  void * vtable;
};

struct Triangle {
  void * vtable;
  double base;
  double height;
};

struct Rectangle {
  void * vtable;
  double width;
  double height;
};

double calcArea_Triangle(Triangle * this) {
  return (this->base * this->base->height) / 2;
}

double calcArea_Rectangle(Rectangle * this) {
  return (this->width * this->height);
}

int main() {
}
